{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Ranked",
        "version": "0.0.2",
        "minimum-overwolf-version": "0.119.1",
        "author": "Ranked Game",
        "icon": "IconMouseOver.png",
        "icon_gray": "IconMouseNormal.png",
        "launcher_icon": "launcher_icon.ico",
        "description": "Join a team and fight your way to the competitive cyberscene!",
        "dock_button_title": "Ranked Game"
    },
    "permissions": ["Extensions", "Streaming", "Profile", "GameInfo", "Clipboard"],
    "data": {
        "start_window": "controller",
        "protocol_override_domains": { "googlesyndication": "http" },
        "developer": {
            "enable_auto_refresh": true,
            "reload_delay": 1000
        },
        "game_targeting": {
            "type": "dedicated",
            "game_ids": [7314, 5426]
        },
        "hotkeys": {
            "toggle_app": {
                "title": "Show/Hide Ranked",
                "action-type": "toggle",
                "default": "Shift+F3"
            }
        },
        "windows": {
            "controller": {
                "file": "controller.html",
                "is_background_page": true,
                "is_alt_f4_blocked": true
            },
            "login": {
                "file": "login.html",
                "transparent": true,
                "disable_rightclick": true,
                "clickthrough": false,
                "resizable": false,
                "is_background_page": false,
                "show_in_taskbar": true,
                "show_minimize": false,
                "is_alt_f4_blocked": true,
                "override_on_update": true,
                "desktop_only": true,
                "native_window": true,
                "size": {
                    "width": 420,
                    "height": 700
                },
                "min_size": {
                    "width": 420,
                    "height": 700
                },
                "max_size": {
                    "width": 420,
                    "height": 700
                },
                "block_top_window_navigation": true,
                "popup_blocker": true,
                "mute": true
            },
            "success": {
                "file": "success.html",
                "is_background_page": true,
                "is_alt_f4_blocked": true
            },
            "app": {
                "file": "index.html",
                "transparent": true,
                "disable_rightclick": true,
                "clickthrough": false,
                "resizable": false,
                "is_background_page": false,
                "show_in_taskbar": true,
                "show_minimize": true,
                "override_on_update": true,
                "is_alt_f4_blocked": true,
                "desktop_only": true,
                "native_window": true,
                "size": {
                    "width": 420,
                    "height": 700
                },
                "min_size": {
                    "width": 420,
                    "height": 700
                },
                "max_size": {
                    "width": 420,
                    "height": 700
                },
                "block_top_window_navigation": true,
                "popup_blocker": true,
                "mute": true
            }
        },
        "game_events": [7314, 5426],
        "force_browser": "overwolf",
        "launch_events": [
            {
                "event": "GameLaunch",
                "event_data": {
                    "game_ids": [7314, 5426, 10902]
                },
                "start_minimized": true
            }
        ]
    }
}
